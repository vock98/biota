<%
    var big_title = "使用紀錄報表";
    var bar_title = "設備操作紀錄報表";
%>
	<div id="wrapper">
        <%- partial("../layout/left") %> 
		<div id="page-wrapper">
			<div class="row">
				<div class="col-lg-12">
					<h1 class="page-header"><%=big_title%></h1>
				</div>
				<!-- /.col-lg-12 -->
			</div>
			<!-- /.row -->
			<div class="row">
				<div class="col-lg-12">
					<div class="panel panel-default">
						<div class="panel-heading">
							<%=bar_title%>
						</div>
						<!-- /.panel-heading -->
						<div class="panel-body">
                            <form role="form" id="form_validation" action="<%=form_action%>">       
                                <%-: {is_require:false, is_autocomplete:true, input_ch:"選擇AP", id_name:"select_ap", input_data: sitename_option , input_extra:null}|create_select %>
                                <button type="submit" id="send_submit" class="btn btn-primary" style="display:none">切換ap</button>
                            </form>
							<div id="morris-line-chart"></div>
						</div>
						<!-- /.panel-body -->
					</div>
					<!-- /.panel -->
				</div>				
			</div>
			<!-- /.row -->
		</div>
		<!-- /#page-wrapper -->
	</div>
	<!-- /#wrapper -->
	<script>
	$(function() {
		continue_line(<%-JSON.stringify(fake_obj)%>);
        check_default('select'       ,'select_ap'     ,'<%-: select_ap %>');
        $("#select_ap").on("change", function(){
            if($("#select_ap :selected").text()!='<%-: select_ap %>'){
                $("#send_submit").click();
            }
        });
	});
	</script>    